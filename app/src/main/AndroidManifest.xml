<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.wolfbang.fsync"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:name="com.wolfbang.fsync.application.FsyncApplication"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar"
        >
        <activity android:name="com.wolfbang.fsync.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.wolfbang.shared.SingleFragActivity"/>
        <activity android:name="com.wolfbang.fsync.feature2.impl.Feature2Activity"/>

    </application>

</manifest>
